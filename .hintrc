{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "input[type=color]"
        ]
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "forced-color-adjust",
          "-webkit-overflow-scrolling"
        ]
      }
    ]
  }
}
